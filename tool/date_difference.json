{
  "name": "date_difference",
  "description": "Calculate the time elapsed from start date until end date. e.g. given start date November 5, 2020 and end date November 9, 2020, the time elapsed in days is 4",
  "strict": true,
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "type": "string",
        "description": "The start date",
        "format": "date-time"
      },
      "end": {
        "type": "string",
        "description": "The end date",
        "format": "date-time"
      },
      "unit": {
        "type": "string",
        "description": "The unit of time in which the difference is given",
        "enum": [
          "Seconds",
          "Minutes",
          "Hours",
          "Days",
          "Weeks",
          "Months",
          "Years"
        ]
      }
    },
    "additionalProperties": false,
    "required": [
      "start",
      "end",
      "unit"
    ]
  }
}
