?start: inputs
?inputs: calculation
  | inputs "&" calculation
?calculation: expression [unit " TO " unit]
?unit: LWORD
  | unit LWORD
?expression: product
  | expression ("+" | "-") product
?product: atom
  | product ("*" | "/") atom
?atom: SIGNED_NUMBER
  | expression
  | ["round" | "floor" | "ceil"] "(" expression ")"
LWORD: /[a-z]+/
%import common.SIGNED_NUMBER
%import common.WS
%ignore WS
